<local:EntityWindow x:TypeArguments="local:WarehouseProvider"
        x:Class="AutoFix.WarehouseProviderWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoFix"
        mc:Ignorable="d"
        Title="Поставщик склада" Width="200" SizeToContent="Height"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen">
    <StackPanel Margin="5">
        <TextBlock>Наименование</TextBlock>
        <TextBox Margin="0 0 0 5" Text="{Binding Name}" />

        <TextBlock>Контактная информация</TextBlock>
        <TextBox Margin="0 0 0 5" Height="50" Text="{Binding ContactInfo}" />

        <Button Padding="3" HorizontalAlignment="Center" Click="SaveButton_Click">Сохранить</Button>
    </StackPanel>
</local:EntityWindow>
